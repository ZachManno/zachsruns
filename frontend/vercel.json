{
  "version": 2,
  "installCommand": "npm install",
  "buildCommand": "cp -r ../backend ./backend && npm run build",
  "outputDirectory": ".next",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    }
  ],
  "functions": {
    "api/index.py": {
      "includeFiles": "backend/**",
      "maxDuration": 30
    }
  }
}

